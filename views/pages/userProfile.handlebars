<br>  <h3 class="text-center">{{type}}</h3>
<hr>
<div class="row">
{{#each resultList}}

<div class="col-md-4 card card-product">

	<figure>
        <div class="headercar">{{this.requested}}</div>
		<div class="img-wrap"><img src="{{this.cloudinaryURL.[0]}}" alt="{{this.itemName}}"></div>
		<figcaption class="info-wrap">
				<h4 class="title">{{this.itemName}}</h4>
				<p class="desc">{{this.itemDescription}}</p>
                <p class="desc">{{this.city}}</p>
				{{!-- <div class="rating-wrap">
					<div class="label-rating">132 reviews</div>
					<div class="label-rating">154 orders </div>
				</div> <!-- rating-wrap.// --> --}}
		</figcaption>
    </figure>
    <div class="bottom-wrap">
        <div class="price-wrap h5">
            <span class="price-new">${{this.price}}</span>
            <form action="/listing/edit/{{this._id}}" method="get">
              <button class="delete">Edit</button>
            </form>
            <form action="/delete/{{this._id}}" method="post">
              <button class="delete">Delete</button>
            </form>
        </div> <!-- price-wrap.// -->
    </div> <!-- bottom-wrap.// -->

</div> <!-- col // -->
{{/each}}


</div> <!-- row.// -->
    </div>
            </div>
                <div class="tab-pane" id="profile">
                    <h4 class="m-y-2">User Profile</h4>
                    <div class="row">
                        <div class="col-md-6">
                            <table class="table table-hover table-striped">
                                <tbody>
                                    <tr>
                                        <td>
                                            <strong>Name</strong>
                                        </td>
                                        <td>
                                            {{person.name}}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>Email-id</strong>
                                        </td>
                                        <td>
                                            {{person.email}}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>Phone No</strong>
                                        </td>
                                        <td>
                                            {{person.phoneNumber}}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>City</strong>
                                        </td>
                                        <td>
                                            {{person.city}}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>State</strong>
                                        </td>
                                        <td>
                                            {{person.state}}
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <strong>Zip</strong>
                                        </td>
                                        <td>
                                            {{person.zip}}
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!--/row-->
                </div>

<!--container.//-->
